- var theme_customizer      = true;
- var morrischart           = true;
- var apex                  = true;
- var apexmoment            = true;
- var dashboard_default     = true;
- var owlcarousel           = true;
- var page_owlcarousel      = true;
- var page_datatable        = true;
- var datatable             = true;
- var page_datatable1       = true;
- var notify                = true;
- var animate               = true;
- var typeahead             = true;
- var typeahead_search      = true;
- var sidebar               = true;
- var scrollbar             = true;
- var wow                   = true;
- var slick                 = true;
- var header_slider         = true;
- var height_equal          = true;
include ../../components/mixin
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        //- include ../../components/toggle-icon
        .page-title.w-auto
          +breadcrumb(['Dashboard','Advanced'],'Advanced Dashboard')
        include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid         
            .page-title
              .row
                include ../../components/sub-header
                +breadcrumb(['Dashboard',' Advanced'],'Advanced Dashboard')  
          // Container-fluid starts
          .container-fluid.dashboard_default
            .row.widget-grid
              .col-xl-6 
                .card.sales-chart
                  .card-header.card-no-border.pb-0 
                    h3 Sales Analytics
                  .card-body
                    ul 
                      li.me-2
                        span.circle.bg-primary.me-1 
                        p Pending
                          span ($4,875)
                      li 
                        span.circle.bg-secondary.me-1
                        p Complete   
                          span ($1,450)
                    .legend 
                      #sales-analytics
                  
          // Container-fluid Ends
        include ../../components/footer        
    include ../../components/footer-files
    script. 
      new WOW().init();