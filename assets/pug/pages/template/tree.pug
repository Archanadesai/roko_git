- var tree                  = true;
- var theme_customizer      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
include ../../components/mixin
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        include ../../components/toggle-icon
        .page-title.w-auto
          +breadcrumb(['Dashboard','Advanced'],'Advanced Dashboard')
        include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid         
            .page-title
              .row
                include ../../components/sub-header
                +breadcrumb(['Dashboard',' Advanced'],'Advanced Dashboard')
          // Container-fluid starts
          .container-fluid
            .row
              .col-sm-6
                .card
                  .card-header.pb-0
                    h3 Basic Tree
                    p.f-m-light.mt-1 
                      | Use the dynamic tree view with checkboxes.
                  .card-body
                    .tree-container
              .col-sm-6
                .card
                  .card-header.pb-0
                    h3 Disabled Tree
                    p.f-m-light.mt-1 
                      | Use the dynamic tree view with checkboxes.
                  .card-body
                    .disabled-container
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
