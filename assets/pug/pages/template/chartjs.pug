- var theme_customizer      = true;
- var chartjs               = true;
- var page_chartjs          = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
include ../../components/mixin
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        
        .page-title.w-auto
          +breadcrumb(['Dashboard','Advanced'],'Advanced Dashboard')
        include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid         
            .page-title
              .row
                include ../../components/sub-header
                +breadcrumb(['Dashboard',' Advanced'],'Advanced Dashboard')
          // Container-fluid starts
          .container-fluid
            .row
              .col-xl-6.col-md-12.box-col-6
                .card
                  .card-header.pb-0
                    h3 Bar Chart
                  .card-body.chart-block
                    canvas#myBarGraph
              .col-xl-6.col-md-12.box-col-6
                .card
                  .card-header.pb-0
                    h3 Line Graph
                  .card-body.chart-block
                    canvas#myGraph
              .col-xl-6.col-md-12.box-col-6
                .card
                  .card-header.pb-0
                    h3 Radar Graph
                  .card-body.chart-block
                    canvas#myRadarGraph
              .col-xl-6.col-md-12.box-col-6
                .card
                  .card-header.pb-0
                    h3 Line Chart
                  .card-body.chart-block
                    canvas#myLineCharts
              .col-xl-6.col-md-12.box-col-6
                .card
                  .card-header.pb-0
                    h3 Doughnut Chart
                  .card-body.chart-block.chart-vertical-center
                    canvas#myDoughnutGraph
              .col-xl-6.col-md-12.box-col-6
                .card
                  .card-header.pb-0
                    h3 Polar Chart
                  .card-body.chart-block.chart-vertical-center
                    canvas#myPolarGraph
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
