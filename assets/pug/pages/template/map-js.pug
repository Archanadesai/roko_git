- var theme_customizer      = true;
//- - var jquery_ui             = true;
- var map_js                = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
include ../../components/mixin
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      .page-header.row
        include ../../components/toggle-icon
        .page-title.w-auto
          +breadcrumb(['Dashboard','Advanced'],'Advanced Dashboard')
        include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid         
            .page-title
              .row
                include ../../components/sub-header
                +breadcrumb(['Dashboard',' Advanced'],'Advanced Dashboard')
          // Container-fluid starts
          .container-fluid
            .row
              .col-xl-6
                .card
                  .card-header.pb-0
                    h3 Map at a specified location
                    span Display a map at a specified location and zoom level.
                  .card-body.z-1
                    #weathermap.map-js-height
              .col-xl-6
                .card
                  .card-header.pb-0
                    h3 Map using View Bounds
                    span Display a map of a given area.
                  .card-body.z-1
                    #map2.map-js-height 
              
          // Container-fluid Ends
        include ../../components/footer
    include ../../components/footer-files
